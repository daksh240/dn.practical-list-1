#include<iostream>
using namespace std;
  
int main() 
{
    char op;
    float num1,num2;
      
    cout<<"Enter an arithemetic operator(+ - * /)"<<endl;
    cin>>op;
    cout<<"Enter two numbers as operands"<<endl;
    cin>>num1 >> num2;
  
    switch(op) 
	{
        case'+': 
                cout<<num1<< " + " << num2 << " = " << num1+num2;
                break;
        case'-':
                cout<<num1<< " - " << num2 << " = " << num1+num2;
                break;
        case'*':
                cout<<num1<< " * " << num2 << " = " << num1*num2;
                break;
        case'/':
                cout<<num1<< " / " << num2 << " = " << num1/num2;
                break;
        default: 
                printf("the error is not support::");
    }

    cout<<"daksh nanera 08";
    return 0;
}
