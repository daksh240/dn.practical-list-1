#include <iostream>  
using namespace std; 
 
int main()
{  
    int num,rem,rev=0,temp;  
    
    cout<<"Enter the Number:: ";    
    cin>>num;  
    
    temp=num;    
    while(temp>0)
	{    
        rem=temp%10;    
        rev=(rev*10)+rem;    
        temp=temp/10;    
    }    
    if(num==rev)
	{ 
        cout<<num<<" this is a Palindrome Number" <<endl; 
	}
    else
    {
	    cout<<num<<" this is not a Palindrome Number" <<endl;  
	}	
 	   return 0;  
}  
